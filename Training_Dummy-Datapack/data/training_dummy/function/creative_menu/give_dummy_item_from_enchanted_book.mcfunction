execute as @a[predicate=training_dummy:creative_menu_item_0] run item replace entity @s hotbar.0 with \
armor_stand[\
item_model="training_dummy:dummy",\
item_name={translate:"block.training_dummy.dummy"},\
rarity="uncommon",\
lore=[[\
{"color": "dark_gray","text": "[ "},\
{"color": "gray","keybind": "key.sneak"},\
{"color": "gray","text": " + "},\
{"color": "gray","keybind": "key.attack"},\
{"color": "dark_gray","text": " ] "},\
{"translate": "block.training_dummy.dummy.tooltip1"}\
]],\
entity_data={\
id:armor_stand,\
Tags: [training_dummy],\
Invisible: true,\
equipment: {head: {id: "paper",components: {"item_model": "training_dummy:dummy"},count: 1}},\
NoBasePlate: true,\
NoGravity: true,\
Marker: true,\
DisabledSlots:4144959\
}]

